{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "ConnectionStrings": {
    "MoviesGalleryCS": "Server=(local);Database=DB_MoviesGallery;Integrated Security=true;"
  },
  "ImageConfiguration": {
    "SaveTo": "Images",
    "AllowedExtensions": ".png, .jpg, .jpeg",
    "MinimumSizeInBytes": 1000,
    "MaximumSizeInBytes": 10000000
  },
  "AllowedHosts": "*"
}
